{
  "name": "@wakey/studio",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm dev:server\" \"pnpm dev:client\"",
    "dev:client": "vite",
    "dev:server": "wrangler dev",
    "build": "vite build && wrangler deploy --dry-run",
    "deploy": "pnpm db:migrate:prod && vite build && wrangler deploy",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply wakey-admin --local",
    "db:migrate:prod": "wrangler d1 migrations apply wakey-admin --remote"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@google/generative-ai": "^0.21.0",
    "@hono/zod-validator": "^0.4.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@tanstack/react-query": "^5.62.0",
    "@wakey/email": "workspace:*",
    "@wakey/tailwind-config": "workspace:*",
    "clsx": "^2.1.0",
    "drizzle-orm": "^0.38.3",
    "googleapis": "^144.0.0",
    "hono": "^4.6.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.1.0",
    "resend": "^4.0.0",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241218.0",
    "@tailwindcss/vite": "^4.1.0",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^9.1.0",
    "drizzle-kit": "^0.30.0",
    "tailwindcss": "^4.1.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "wrangler": "^3.99.0"
  }
}
