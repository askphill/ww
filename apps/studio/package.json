{
  "name": "@wakey/studio",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm dev:server\" \"pnpm dev:client\"",
    "dev:client": "vite",
    "dev:server": "wrangler dev",
    "build": "vite build && wrangler deploy --dry-run",
    "deploy": "vite build && wrangler deploy",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply wakey-admin --local",
    "db:migrate:prod": "wrangler d1 migrations apply wakey-admin --remote"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@hono/zod-validator": "^0.4.0",
    "@tanstack/react-query": "^5.62.0",
    "clsx": "^2.1.0",
    "drizzle-orm": "^0.38.3",
    "googleapis": "^144.0.0",
    "hono": "^4.6.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.1.0",
    "resend": "^4.0.0",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241218.0",
    "@tailwindcss/vite": "^4.1.0",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^9.1.0",
    "drizzle-kit": "^0.30.0",
    "tailwindcss": "^4.1.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "wrangler": "^3.99.0"
  }
}
