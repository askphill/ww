{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "typecheck": {
      "dependsOn": ["^build"]
    },
    "fetch": {
      "cache": false
    },
    "analyze": {
      "cache": false
    },
    "generate": {
      "cache": false
    },
    "track": {
      "cache": false
    },
    "sync": {
      "cache": false
    }
  }
}
