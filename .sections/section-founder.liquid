{%- liquid
  assign s = section.settings
  assign founder_image_alt = s.image.alt | default: s.title | default: shop.name
  assign founder_image_alt_escaped = founder_image_alt | strip_html | escape
-%}

<div class="founder--col ">
  {{
    s.image
    | image_url: width: 3840
    | image_tag:
      width: s.image.width,
      class: 'founder--image rounded',
      sizes: '(max-width: 749px) 100vw, 50vw',
      widths: '375, 550, 750, 1100, 1500, 1780',
      alt: founder_image_alt_escaped,
      fetchpriority: 'auto'
  }}
</div>
<div class="founder--col rounded">
  <h2 class="founder--title">
    {{- s.title -}}
  </h2>
  <div class="founder--body">
    <div class="founder--text">
      {{- s.text -}}
    </div>
    <div>
      <svg
        class="founder--authograph"
        viewBox="0 0 275 173"
        fill="none"
        xmlns="http://www.w3.org/2000/svg"
        xmlns:xlink="http://www.w3.org/1999/xlink"
      >
        <rect width="275" height="172.143" fill="url(#pattern0)"/>
        <defs>
        <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
        <use xlink:href="#image0_628_1352" transform="scale(0.0025974 0.00414938)"/>
        </pattern>
        <image id="image0_628_1352" width="385" height="241" xlink:href="data:image/png;base64,"/>
        </defs>
      </svg>
    </div>
    <div class="founder--founder">Juliette, Founder</div>
  </div>
</div>

{% stylesheet %}
.founder {
position: relative;
}

.founder--col {
position: relative;
display: flex;
flex-direction: column;
}

.founder--image {
height: 100%;
width: 100%;
object-fit: cover;
}

.founder--col:last-child {
padding: calc(var(--padding) * 2);
background-color: var(--color-softorange);
}

.founder--body {
padding: calc(var(--padding) * 5) 0 calc(var(--padding) * 2)
  calc(var(--padding) * 3);
}

.founder--authograph {
padding-top: calc(var(--padding) * 4);
padding-bottom: 1rem;
width: 10rem;
}

@media (width >= 47rem) {
.founder {
  display: grid;
  display: grid;
  grid-template-columns: repeat(2, 1fr);
}
.founder--image {
  height: 100vh;
  width: 100%;
  object-fit: cover;
  position: -webkit-sticky;
  position: sticky;
  top: 0;
}

.founder--col:last-child {
  padding: calc(var(--padding) * 2) calc(var(--padding) * 5)
    calc(var(--padding) * 3) calc(var(--padding) * 2);
}
.founder--title {
  font-size: 2.5rem;
  line-height: 1.1;
  max-width: 35rem;
}

.founder--body {
  padding: calc(var(--padding) * 5) 0 calc(var(--padding) * 2)
    calc(var(--padding) * 6);
  max-width: 34.38rem;
}
}
{% endstylesheet %}

{% schema %}
{
  "name": "Founder",
  "class": "founder mt--1",
  "settings": [
    {
      "type": "image_picker",
      "id": "image",
      "label": "image"
    },
    {
      "type": "richtext",
      "id": "title",
      "label": "Title"
    },
    {
      "type": "richtext",
      "id": "text",
      "label": "Text"
    }
  ],
  "presets": [
    {
      "name": "Founder"
    }
  ]
}
{% endschema %}
